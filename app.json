{
  "name": "goflyway-heroku",
  "description": "deploy goflyway to Heroku.",
  "keywords": ["goflyway"],
  "env": {
    "VER": {
       "description": "输入 goflyway 版本号",
       "value": "2.0.0rc1"
     }, 
     "KEY": {
       "description": "密码，服务端与客户端需保持一致",
       "value": "30522b10-a8d2-11e8-978c-f0d5bf44df99"
     },
     "PORT": {
       "description": "设置监听端口，该地址是客户端需要连接的端口",
       "value": "6666"
     },
     "LEVEL": {
       "description": "日志输出层级，有：dbg0，dbg，log，info，warn，err，off（关闭输出）",
       "value": "log"
     }
  },
  "website": "https://github.com/wizos/goflyway-heroku",
  "repository": "https://github.com/wizos/goflyway-heroku",
  "stack": "container"
 }
